{
  "name": "github-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "server": "node --loader ts-node/esm src/app.js",
    "lint": "npm-run-all --continue-on-error --aggregate-output --parallel \"lint:*\"",
    "lint:docs:fix": "npm run lint:docs -- --fix",
    "lint:js": "eslint --cache .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:package-json-sorting": "sort-package-json --check",
    "lint:package-json-sorting:fix": "sort-package-json package.json"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "octokit": "^2.0.14",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "eslint-plugin-square": "^25.0.0",
    "npm-run-all": "^4.1.5",
    "smee-client": "^1.2.3",
    "sort-package-json": "^2.4.1",
    "vite": "^4.1.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
